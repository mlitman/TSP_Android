<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.awesomefat.tsp.MainActivity"
    android:orientation="vertical">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/graphJSONET"
        android:layout_gravity="center_horizontal"
        android:hint="Node Names"
        android:gravity="center"
        android:text="{&quot;nodes&quot;:[{&quot;name&quot;:&quot;A&quot;},{&quot;name&quot;:&quot;B&quot;},{&quot;name&quot;:&quot;C&quot;},{&quot;name&quot;:&quot;D&quot;},{&quot;name&quot;:&quot;E&quot;}],&quot;edges&quot;:[{&quot;node1&quot;:&quot;A&quot;, &quot;node2&quot;:&quot;B&quot;,&quot;value&quot;:15},{&quot;node1&quot;:&quot;A&quot;, &quot;node2&quot;:&quot;C&quot;,&quot;value&quot;:9},{&quot;node1&quot;:&quot;B&quot;, &quot;node2&quot;:&quot;C&quot;,&quot;value&quot;:10},{&quot;node1&quot;:&quot;B&quot;, &quot;node2&quot;:&quot;D&quot;,&quot;value&quot;:8},{&quot;node1&quot;:&quot;C&quot;, &quot;node2&quot;:&quot;D&quot;,&quot;value&quot;:7},{&quot;node1&quot;:&quot;D&quot;, &quot;node2&quot;:&quot;E&quot;,&quot;value&quot;:28}]}"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate Graph"
        android:id="@+id/button"
        android:layout_gravity="center_horizontal"
        android:onClick="generateGraphButtonPressed"/>
</LinearLayout>
